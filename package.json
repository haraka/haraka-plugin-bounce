  "dependencies": {
    "address-rfc2821": "^2.1.2",
    "haraka-net-utils": "^1.6.0",
    "haraka-plugin-spf": "^1.2.5",
    "haraka-tld": "^1.2.1"
  },
  "devDependencies": {
